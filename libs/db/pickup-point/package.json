{
  "name": "pickup-point-db",
  "version": "0.0.1",
  "private": true,
  "exports": {
    "./client": {
      "types": "./esm/client.ts",
      "import": "./esm/client.ts",
      "require": "./cjs/client.ts"
    },
    "./browser": {
      "types": "./esm/browser.ts",
      "import": "./esm/browser.ts",
      "require": "./cjs/browser.ts"
    },
    "./models": {
      "types": "./esm/models.ts"
    }
  },
  "files": [
    "prisma"
  ],
  "nx": {
    "sourceRoot": "libs/db/pickup-point/src",
    "targets": {
      "build:cjs": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "swcrc": "libs/db/pickup-point/.cjs.swcrc",
          "outputPath": "libs/db/pickup-point/dist-cjs",
          "main": "libs/db/pickup-point/src/index.ts",
          "tsConfig": "libs/db/pickup-point/tsconfig.cjs.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      },
      "build:esm": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "swcrc": "libs/db/pickup-point/.esm.swcrc",
          "outputPath": "libs/db/pickup-point/dist-esm",
          "main": "libs/db/pickup-point/src/index.ts",
          "tsConfig": "libs/db/pickup-point/tsconfig.esm.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      },
      "prisma:pull": {
        "executor": "nx:run-commands",
        "options": {
          "command": "prisma db pull --schema=./prisma/schema.prisma",
          "cwd": "libs/db/pickup-point"
        }
      },
      "prisma:generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "prisma generate --schema=./prisma/schema.prisma",
          "cwd": "libs/db/pickup-point"
        }
      },
      "ci": {
        "dependsOn": [
          "build"
        ],
        "executor": "@nx-tools/nx-container:build",
        "options": {
          "push": true,
          "metadata": {
            "images": [
              "ghcr.io/nochlezhka/Bot-for-Delivery/pickup-point-db"
            ],
            "tags": [
              "type=ref,event=branch"
            ]
          },
          "platforms": [
            "linux/amd64",
            "linux/arm64"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  }
}
